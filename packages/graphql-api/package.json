{
  "name": "graphql-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "watch": "concurrently --kill-others \"npm run --watch gen-schema-types\" \"npm run server\"",
    "build": "rm -rf ./dist && tsc && copyfiles -u 1 src/**/*.graphql dist/server/src",
    "start": "node dist/index.js",
    "server": "ts-node-dev --inspect --respawn --transpileOnly ./src/index.ts",
    "server2": "nodemon --inspect=5858 -r ts-node/register ./src/index.ts",
    "gen-schema-types": "nodemon -e graphql --exec ts-node src/scripts/createTypes.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "daveyedwards",
  "license": "MIT",
  "devDependencies": {
    "@gql2ts/from-schema": "^1.10.1",
    "@gql2ts/types": "^1.9.0",
    "@types/express": "^4.17.0",
    "@types/graphql-fields": "^1.3.0",
    "@types/graphql-type-json": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "concurrently": "^4.1.1",
    "copyfiles": "^2.0.0",
    "eslint": "^6.3.0",
    "nodemon": "^1.19.1",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@google-cloud/debug-agent": "^4.0.1",
    "@myiworlds/cloud-services": "^1.0.0",
    "@myiworlds/credentials": "^1.0.0",
    "@myiworlds/types": "^1.0.0",
    "apollo-server": "^2.7.0",
    "apollo-server-express": "^2.8.1",
    "cookie": "^0.4.0",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-bigint": "^1.0.0",
    "graphql-fields": "^2.0.3",
    "graphql-tools": "^4.0.5",
    "graphql-type-json": "^0.3.0",
    "merge-graphql-schemas": "^1.6.2-bdaf070.0"
  }
}
